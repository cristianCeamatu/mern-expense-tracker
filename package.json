  },
  "homepage": "https://github.com/cristianCeamatu/mern-expense-tracker#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "mongoose": "^5.9.17"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
